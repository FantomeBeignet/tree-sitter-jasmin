u32[2] g = {1,2};

param int n = 1500;

export fn test (reg u32 x) -> reg u32 {
  inline int i;
  reg ptr u32[2] pg;
  reg u32 y; 

  pg = g;
  for i = 0 to n {
    y = pg[1];
    x += y; 
  } 

  for i = 0 to n {
    y = g[1]; 
    x += y;
  }

  for i = 0 to n {
    y = g[0]; 
    x += y; 
  } 

  return x;
}

  
